	Si num = 0 entonces 
		Escribir "El numero no es par ni impar "
	Sino 
		Si num MOD 2 = 0 y num <>0 entonces 
			Escribir "El numero ingresado es par  "
		sino 
		    Escribir "El numero ingresado es impar"
		FinSi
	FinSi
